#+ endpoint: http://fuseki:3030/TOOP/query
#+ method: GET
#+ summary: select the pairs of concepts in source and target namespaces that have an equivalent class relation

PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>

SELECT ?s ?o
WHERE {
    ?s ?p ?o .
    FILTER (?p = owl:equivalentClass) .
    FILTER (STRSTARTS(str(?s),?_sourcenamespace)) .
    FILTER (STRSTARTS(str(?o),?_targetnamespace)).
}